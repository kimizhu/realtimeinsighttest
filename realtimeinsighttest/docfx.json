{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/realtimeinsighttest/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"
    },
    "fileMetadata": {
          "articles/*.md": "https://feedback.azure.com/forums/34192--general-feedback"
	    },
	    "searchScope": {
	       "articles/data-lake-analytics/**/*.md": ["Azure","Azure Data Lake Analytics"]
	    }
    },
    "template": [],
    "dest": "realtimeinsighttest",
    "markdownEngineName": "markdig"
  }
}
